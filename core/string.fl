/*
  due a bug (padding), encoding cannot be i8,
  it's still compatible with the c version...
*/

struct string {
  i32 length,
  i32 used,
  u32 capacity,
  i32 encoding,
  vector<i8> value
};

/*
fn autocast(ptr<i8> str) : string {
  return st_newc(str, st_enc_ascii)
}
*/

fn autocast(string str) : ptr<i8> {
  return str.value;
}
