var i;

printf("for\n");

for i = 0; i < 10; i = i + 1 {
  printf("%ld-", i);
}
printf("\n");

for i = 5; i <= 10; i = i + 2 {
  printf("%ld-", i);
}

printf("\nwhile\n");

i = 0;
while i < 10 {
  printf("%ld-", i);
  i = i + 1;
}
printf("\n");

i = 5;
while i <= 10 {
  printf("%ld-", i);
  i = i + 2;
}
printf("\ndowhile\n");

i = 0;
do {
  printf("%ld-", i);
  i = i + 1;
} while i < 10;


printf("\n");
i = 5;
do {
  printf("%ld-", i);
  i = i + 2;
} while i <= 10;
printf("\n");
