/* polymorphism */
function print(ptr(i8) str) : i32 {
  return printf("as string = %s\n", str);
}

function print(i32 num) : i32 {
  return printf("as i32 %d\n", num);
}

function print(f32 f) : i32 {
  return printf("as f32 %f\n", f);
}


print(10);
