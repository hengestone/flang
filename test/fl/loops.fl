var i;

printf("for\n");

for i = 0; i < 10; i = i + 1 {
  printf("%d-", i);
}
printf("\n");

for i = 5; i <= 10; i = i + 2 {
  printf("%d-", i);
}

printf("\nwhile\n");

i = 0;
while i < 10 {
  printf("%d-", i);
  i = i + 1;
}
printf("\n");

i = 5
while i <= 10 {
  printf("%d-", i);
  i = i + 2;
}
printf("\n");
