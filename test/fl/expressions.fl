var f64 x;
var f64 y;
var f32 z;

x = 10;
y = 5;
z = 3;

var i32 left;
var i32 right;

left = 65;
right = 129;

printf("1. x + y = %f\n", x + y);
printf("2. x - y = %f\n", x - y);
printf("3. x * y = %f\n", x * y);
printf("4. x / y = %f\n", x / y);
printf("5. x / y = %f\n", x / y);
printf("6. y / x = %f\n", y / x);
printf("7. y / x = %f\n", y / x);
printf("8. x %% y = %f\n", x % y);
printf("9. y %% x = %f\n", y % x);

printf("10. left %% right = %d\n", left % right);
printf("11. right %% left = %d\n", right % left);

printf("12. x * left = %f\n", x * left);
printf("13. left * x = %f\n", left * x);


var i32 i5;
i5 = 5;
var i32 i3;
i3 = 3;
printf("14. i5 << i3 = %d\n", i5 << i3);
printf("15. (i5 << i3) >> i3 = %d\n", (i5 << i3) >> i3);
printf("16. (i5 << i3) >> i3 = %d\n", i5 >> i3);

